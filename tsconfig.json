{
  "compilerOptions": {
    "lib": [
      "es5",
      "es2015",
      "es2016",
      "es2017",
      "dom"
    ],
    "strictNullChecks": true,
    "jsx": "react",
    "allowJs": true,
    "sourceMap": true,
    "inlineSources": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2015",
    "downlevelIteration": true
  },
  "include": [
    "./client",
  ],
  "exclude": [
    "./node_modules",
  ],
  "compileOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  }
}